alias gst git status
alias git git --no-pager $*
alias la ls -la $*
alias ll ls -la $*
alias fr find-file-other-frame $1
alias fw find-file-other-window $1
alias ff find-file $1
alias oo find-file-other-window $1
alias o emacs $*
alias clear erase-buffer
alias emacs for i in ${eshell-flatten-list $*} {find-file $i}
alias reload eshell-read-aliases-list
